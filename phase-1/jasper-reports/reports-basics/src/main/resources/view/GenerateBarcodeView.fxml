<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.dep10.app.controller.GenerateBarcodeViewController">
   <children>
      <Label alignment="CENTER" layoutX="282.0" layoutY="32.0" text="Generate barcodes" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="txtText" layoutX="203.0" layoutY="101.0" promptText="Enter Something to generate barcode" AnchorPane.leftAnchor="150.0" AnchorPane.rightAnchor="150.0" />
      <ImageView fx:id="imgBarcode" fitHeight="117.0" fitWidth="300.0" layoutX="153.0" layoutY="151.0" pickOnBounds="true" preserveRatio="true" AnchorPane.leftAnchor="150.0" AnchorPane.rightAnchor="150.0" />
      <Button fx:id="btnDownloadBarcode" layoutX="272.0" layoutY="298.0" mnemonicParsing="false" onAction="#btnDownloadBarcodeOnAction" text="Download Barcode" AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="200.0" />
   </children>
</AnchorPane>
